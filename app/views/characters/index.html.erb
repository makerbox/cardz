<p id="notice"><%= notice %></p>

<h1>Characters</h1>

<table>
  <thead>
    <tr>
      <th>Health</th>
      <th>Image</th>
      <th>Attack</th>
      <th>Defense</th>
      <th>Action</th>
      <th>Name</th>
      <th>Value</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @characters.each do |character| %>
      <tr>
        <td><%= character.health %></td>
        <td><%= character.image %></td>
        <td><%= character.attack %></td>
        <td><%= character.defense %></td>
        <td><%= character.action %></td>
        <td><%= character.name %></td>
        <td><%= character.value %></td>
        <td><%= character.profile %></td>
        <td><%= link_to 'Show', character %></td>
        <td><%= link_to 'Edit', edit_character_path(character) %></td>
        <td><%= link_to 'Destroy', character, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Character', new_character_path %>
